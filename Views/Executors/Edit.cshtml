@model MyProject.Models.Executor

<h1>Изменить исполнителя</h1>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="Name" class="control-label">ФИО исполнителя</label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber" class="control-label">Номер телефона</label>
        <input asp-for="PhoneNumber" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить" class="btn btn-primary" />
    </div>
</form>
<a href="@Url.Action("Index")" class="btn btn-secondary">Назад</a>
